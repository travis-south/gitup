{
    "directories": ["src/", "bin/"],
    "finder": [
        {
            "name": "*",
            "exclude": [
                ".gitignore",
                ".md",
                "phpunit",
                "Tester",
                "Tests",
                "tests",
                "yaml"
            ],
            "in": "vendor"
        }
    ],
    "compactors": [
        "Herrera\\Box\\Compactor\\Json",
        "Herrera\\Box\\Compactor\\Php"
    ],
    "compression": "GZ",
    "git-version": "package_version",
    "main": "bin/gitup",
    "output": "gitup.phar",
    "stub": true,
    "chmod": "0755"
}
